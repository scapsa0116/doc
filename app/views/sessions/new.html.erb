<h1>Log In Please</h1>

<%= flash[:message] if flash[:message] %>
<%= form_for(@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username, required:true %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Log In" %>
    
    <% end %>